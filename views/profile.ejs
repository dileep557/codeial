

<link rel="stylesheet" href="/css/user_profile.css">
    <h1>codeial  social media profile page</h1>



    <% if(user.id==user_profile.id) { %>
        <form action="/users/update/<%= user_profile.id %>"   method="post">
            <input type="text" name="name" placeholder="enter user name " value="<%= user_profile.name %>" required>
            <input type="email" name="email"  placeholder="enter email" value="<%= user_profile.email %>" required>
            <input type="submit" value="update">


        </form>
    <% } else { %>
       <h1> <p> <%= user_profile.name %></p></h1>
       <h1> <p> <%= user_profile.email %></p></h1>

   <% } %>


  

  





   
